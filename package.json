{
  "name": "bryanmylee.com",
  "description": "My personal website",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p watch:css watch:sapper",
    "watch:css": "postcss src/assets/global.css -o static/global.css -w",
    "watch:sapper": "PORT=53000 sapper dev --no-hot --dev-port=53100 --ext '.svelte .svx'",
    "build": "run-s build:css build:sapper",
    "build:css": "NODE_ENV=production postcss src/assets/global.css -o static/global.css",
    "build:sapper": "NODE_ENV=production sapper build --ext '.svelte .svx'",
    "export": "run-s build:css export:sapper",
    "export:sapper": "sapper export --ext '.svelte .svx'",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "deploy": "run-s export deploy:gh",
    "deploy:gh": "node ./scripts/deploy.js"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.3.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "autoprefixer": "^10.1.0",
    "chroma-js": "^2.1.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "mdsvex": "^0.8.9",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-nested": "^5.0.3",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^2.9.0",
    "svelte-preprocess": "^4.6.1",
    "svg-url-loader": "^7.1.1",
    "tailwindcss": "^2.0.2",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "tailwindcss-pseudo-elements": "^1.5.1",
    "ts-loader": "^8.0.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "webpack": "^4.7.0",
    "webpack-modules": "^1.0.0"
  }
}
